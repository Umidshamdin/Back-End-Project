@model Notice
@{
    ViewData["Title"] = "Create";

}


<div class="container mt-5">

    <form class="forms-sample" method="post" asp-action="Create" enctype="multipart/form-data">
        <div class="form-group">
            <label asp-for="Time">Header</label>
            <input class="form-control" style="width:300px" asp-for="Time">
            <span class="text-danger" asp-validation-for="Time"></span>
        </div>
        <div class="form-group">
            <label asp-for="Description">Description</label>
            <input class="form-control" style="width:300px" asp-for="Description">
            <span class="text-danger" asp-validation-for="Description"></span>
        </div>
        <button type="submit" class="btn btn-primary me-2">Submit</button>
        <a asp-action="Index" class="btn btn-light">Cancel</a>
    </form>


</div>

