@model Course
@{
    ViewData["Title"] = "Create";

}

<div class="container mt-5">
    <form class="forms-sample" method="post" asp-action="Create" enctype="multipart/form-data">
        <div class="form-group">
            <label asp-for="Photo"></label>
            <input class="form-control" style="width:300px" asp-for="Photo">
            <span class="text-danger" asp-validation-for="Photo"></span>
        </div>
        <div class="form-group">
            <label asp-for="Name"></label>
            <input class="form-control" style="width:300px" asp-for="Name">
            <span class="text-danger" asp-validation-for="Name"></span>
        </div>
        <div class="form-group">
            <label asp-for="About"></label>
            <input class="form-control" style="width:300px" asp-for="About">
            <span class="text-danger" asp-validation-for="About"></span>
        </div>
        <div class="form-group">
            <label asp-for="Apply"></label>
            <input class="form-control" style="width:300px" asp-for="Apply">
            <span class="text-danger" asp-validation-for="Apply"></span>
        </div>
        <div class="form-group">
            <label asp-for="Certification"></label>
            <input class="form-control" style="width:300px" asp-for="Certification">
            <span class="text-danger" asp-validation-for="Certification"></span>
        </div>

        <div class="form-group">
            <label asp-for="Feature.StudentCount"></label>
            <input class="form-control" style="width:300px" asp-for="Feature.StudentCount">
            <span class="text-danger" asp-validation-for="Feature.StudentCount"></span>
        </div>
        <div class="form-group">
            <label asp-for="Feature.Time"></label>
            <input class="form-control" style="width:300px" asp-for="Feature.Time">
            <span class="text-danger" asp-validation-for="Feature.Time"></span>
        </div>
        <div class="form-group">
            <label asp-for="Feature.Duration"></label>
            <input class="form-control" style="width:300px" asp-for="Feature.Duration">
            <span class="text-danger" asp-validation-for="Feature.Duration"></span>
        </div>
        <div class="form-group">
            <label asp-for="Feature.ClassDuration"></label>
            <input class="form-control" style="width:300px" asp-for="Feature.ClassDuration">
            <span class="text-danger" asp-validation-for="Feature.ClassDuration"></span>
        </div>
        <div class="form-group">
            <label asp-for="Feature.Level"></label>
            <input class="form-control" style="width:300px" asp-for="Feature.Level">
            <span class="text-danger" asp-validation-for="Feature.Level"></span>
        </div>
        <div class="form-group">
            <label asp-for="Feature.Language"></label>
            <input class="form-control" style="width:300px" asp-for="Feature.Language">
            <span class="text-danger" asp-validation-for="Feature.Language"></span>
        </div>
      


        <div class="form-group">
            <label asp-for="Description"></label>
            <input class="form-control" style="width:300px" asp-for="Description">
            <span class="text-danger" asp-validation-for="Description"></span>
        </div>
        <div class="form-group">
            <label asp-for="Feature.Assesments"></label>
            <input class="form-control" style="width:300px" asp-for="Feature.Assesments">
            <span class="text-danger" asp-validation-for="Feature.Assesments"></span>
        </div>

        <button type="submit" class="btn btn-primary me-2">Submit</button>
        <a asp-action="Index" class="btn btn-light">Cancel</a>
    </form>
</div>


